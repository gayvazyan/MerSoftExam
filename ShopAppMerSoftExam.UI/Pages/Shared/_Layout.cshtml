<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewData["Title"]</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">

    <environment include="Development">
        <!-- VENDOR CSS -->
        <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
        <!-- Icons-->
        <link href="~/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
        <link href="~/lib/coreui/icons/css/coreui-icons.css" rel="stylesheet" />
        <link href="~/lib/flag-icon-css/css/flag-icon.css" rel="stylesheet" />
        <link href="~/lib/simple-line-icons/css/simple-line-icons.css" rel="stylesheet" />
        <link href="~/lib/pace-progress/css/pace.css" rel="stylesheet" />
        <!-- MAIN CSS -->
        <link href="~/lib/coreui/css/style.css" rel="stylesheet" />

        <link rel="stylesheet" href="~/css/site.css" />

        <link rel="stylesheet" href="~/lib/select2/select2.min.css">

    </environment>
    <environment exclude="Development">
        <!-- VENDOR CSS -->
        <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
        <link href="~/lib/coreui/icons/css/coreui-icons.min.css" rel="stylesheet" />
        <link href="~/lib/flag-icon-css/css/flag-icon.css" rel="stylesheet" />
        <link href="~/lib/simple-line-icons/css/simple-line-icons.css" rel="stylesheet" />
        <link href="~/lib/pace-progress/css/pace.css" rel="stylesheet" />
        <!-- MAIN CSS -->
        <link href="~/lib/coreui/css/style.min.css" rel="stylesheet" />

        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />

        <link rel="stylesheet" href="~/lib/select2/select2.min.css">

    </environment>

    @RenderSection("Headers", required: false)

    <!-- ICONS -->
    <link rel="icon" type="image/x-icon" sizes="96x96" href="/images/logo.gif">
</head>

<!-- BODY -->
<body class="app header-fixed sidebar-fixed aside-menu-fixed sidebar-lg-show">

    @*<partial name="_CookieConsentPartial" />*@

    <!-- NAVBAR -->
    <partial name="_NavBar" />
    <!-- END NAVBAR -->

    <div class="app-body">

        <!-- LEFT SIDEBAR -->
        <partial name="_SideBar" />
        <!-- END LEFT SIDEBAR -->
        <!-- MAIN CONTENT -->

        <main class="main">


            @RenderBody()
        </main>
        <!-- END MAIN CONTENT -->
    </div>

    <footer class="app-footer">
        <div>
            <span>&copy; @DateTime.Now.Year </span>
            <span>BBBBBBBB</span>

        </div>
        <div class="ml-auto">
            <span>AAAAA:</span>
        </div>
    </footer>

    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
        <!-- CoreUI and necessary plugins-->
        <script src="~/lib/pace-progress/js/pace.min.js"></script>

        <script src="~/lib/coreui/dist/js/coreui.js"></script>

        <script src="~/js/site.js" asp-append-version="true"></script>

        <script src="~/lib/select2/select2.min.js"></script>


    </environment>
    <environment exclude="Development">
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <!-- CoreUI and necessary plugins-->
        <script src="~/lib/pace-progress/pace.min.js"></script>

        <script src="~/lib/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>
        <script src="~/lib/coreui/dist/js/coreui.min.js"></script>

        <script src="~/js/site.min.js" asp-append-version="true"></script>

        <script src="~/lib/select2/select2.min.js"></script>

    </environment>

    @RenderSection("Scripts", required: false)

</body>
</html>
